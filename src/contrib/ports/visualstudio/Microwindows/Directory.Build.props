<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <OutDir>$(SolutionDir)Output\bin\$(Platform)\$(Configuration)\</OutDir>
    <OutDirNoSlash>$(SolutionDir)Output\bin\$(Platform)\$(Configuration)</OutDirNoSlash>
    <IntDir>$(SolutionDir)Output\obj\$(Platform)\$(Configuration)\$(MSBuildProjectName)\</IntDir>
    <IntermediateOutputPath>$(IntDir)</IntermediateOutputPath>
  </PropertyGroup>

  <ItemDefinitionGroup>
    <Link>
      <SuppressStartupBanner>false</SuppressStartupBanner>
    </Link>
    <ClCompile Condition="!$(Configuration.Contains('Wasm'))">
      <PreprocessorDefinitions>%(PreprocessorDefinitions);X11_RGBTXT="/usr/share/X11/rgb.txt";NO_SETENV=1;WINEXTRA=1;WIN32DLL=1;EXPORTS=1;HAVE_FREETYPE_2_SUPPORT=1;_CRT_SECURE_NO_WARNINGS;NOMAIN=1;HAVE_MMAP=0;HAVE_SIGNAL=0;HAVE_FILEIO=1;MW_FEATURE_TIMERS=1;UPDATEREGIONS=1;ERASEMOVE=1;HAVE_BMP_SUPPORT=1;SCREEN_PIXTYPE=MWPF_TRUECOLORARGB;SCREEN_WIDTH=800;SCREEN_HEIGHT=800</PreprocessorDefinitions>
      <SuppressStartupBanner>false</SuppressStartupBanner>
    </ClCompile>
    <ClCompile Condition="$(Configuration.Contains('Wasm'))">
      <PreprocessorDefinitions>%(PreprocessorDefinitions);X11_RGBTXT="/usr/share/X11/rgb.txt";WINEXTRA=1;HAVE_FREETYPE_2_SUPPORT=1;NOMAIN=1;HAVE_MMAP=0;HAVE_SIGNAL=0;HAVE_FILEIO=1;MW_FEATURE_TIMERS=1;UPDATEREGIONS=1;ERASEMOVE=1;HAVE_BMP_SUPPORT=1;SCREEN_PIXTYPE=MWPF_TRUECOLORARGB;SCREEN_WIDTH=800;SCREEN_HEIGHT=800</PreprocessorDefinitions>
      <SuppressStartupBanner>false</SuppressStartupBanner>
    </ClCompile>
    <Lib>
      <SuppressStartupBanner>false</SuppressStartupBanner>
    </Lib>
  </ItemDefinitionGroup>

  <Import Project="$([MSBuild]::GetPathOfFileAbove($(MSBuildThisFile), $(MSBuildThisFileDirectory)..))" 
          Condition="Exists($([MSBuild]::GetPathOfFileAbove($(MSBuildThisFile), $(MSBuildThisFileDirectory)..)))" />


</Project>
